{
  "name": "sample-webpack-config",
  "version": "0.0.0",
  "description": "An example WebPack config setup",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-server": "webpack-dev-server --config config/dev.webpack.config.js --progress --colors --port 8090 --inline",
