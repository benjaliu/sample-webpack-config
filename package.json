    "build": "webpack --config config/production.webpack.config.js --progress --colors --profile",
    "serve": "node_modules/babel-core/bin/babel-node server.js"
  },
  "author": "JC <jorge.e.colindres@gmail.com>",
  "dependencies": {
    "autoprefixer-loader": "~1.1.0",
