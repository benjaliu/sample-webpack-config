    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.3.8",
    "jade": "^1.9.2",
    "jsonfile": "^2.0.0",
    "react": "^0.13.1",
    "react-hot-loader": "^1.1.5",
    "style-loader": "^0.8.3",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
